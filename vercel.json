{
    "version": 2,
    "builds": [
        {
            "src": "api/main.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "Access-Control-Allow-Credentials": "true"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "api/main.js"
        }
    ]
}