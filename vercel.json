{
    "version": 2,
    "builds": [
        {
        "src": "dist/src/main.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "headers": {
                "Access-Control-Allow-Origin": "https://parlink.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST, PUT, PATCH, OPTIONS, DELETE, HEAD, CONNECT, TRACE",
                "Access-Control-Allow-Headers": "Content-Type, Origin, X-Requested-With, Accept, Authorization, refresh_token",
                "Access-Control-Allow-Credentials": "true"
            },
            "dest": "dist/src/main.js"
        }
    ],
    "functions": {
        "dist/src/main.js": {
        "maxDuration": 30,
        "memory": 512
        }
    }
}