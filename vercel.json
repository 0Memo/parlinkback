{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/main.js",
            "use": "@vercel/node"
        }
    ],
    "hooks": {
        "build": {
            "command": "npm install && npx prisma generate && npm run build"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}